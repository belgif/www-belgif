@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix eif: <http://www.belgif.be/id/eif3> .
@prefix eif-level: <http://www.belgif.be/id/eif3/level/> .
@prefix eif-principle: <http://www.belgif.be/id/eif3/principle/> .
@prefix eif-recommendation: <http://www.belgif.be/id/eif3/recommendation/> .


eif-level:legal a skos:Concept ;
	skos:prefLabel "Legal Interoperability"@en, "Juridisch"@nl ;
	skos:definition "Ensuring that organisations operating under different legal frameworks, policies and strategies are able to work together."@en ;
	skos:related eif-recommendation:legislation ;
	skos:inScheme eif-level:  .

eif-level:organisational a skos:Concept ;
	skos:prefLabel "Organisational Interoperability"@en, "Organisatorisch"@nl ;
	skos:definition "Documenting and integrating or aligning business processes and relevant information exchanged."@en ;
	skos:related eif-recommendation:processalignment, eif-recommendation:relationships ;
	skos:inScheme eif-level: .
	
eif-level:semantic a skos:Concept ;
	skos:prefLabel "Semantic Interoperability"@en, "Semantisch"@nl ;
	skos:definition "Ensuring that the precise format and meaning of exchanged data and information is preserved and understood throughout exchanges between parties."@en ;
	skos:related eif-recommendation:datamanagement, eif-recommendation:informationmanagement, eif-recommendation:communities ;
	skos:inScheme eif-level: .
	
eif-level:technical a skos:Concept ;
	skos:prefLabel "Technical Interoperability"@en, "Technisch"@nl ;
	skos:definition "Interface specifications, interconnection and data integration services, data presentation and secure communication protocols."@en ;
	skos:related eif-recommendation:openspecifications2 ;
	skos:inScheme eif-level: .

eif-level:governance a skos:Concept ;
	skos:prefLabel "Interoperability Governance"@en ;
	skos:definition "Decisions on interoperability frameworks, roles and responsibilities, agreements and other aspects of ensuring and monitoring interoperability at national and EU levels."@en ;
	skos:related eif-recommendation:holisticgovernance, eif-recommendation:standardsprocess, eif-recommendation:standardsassessment, eif-recommendation:standardscatalogues, eif-recommendation:standardsparticipation ;
	skos:inScheme eif-level: .

eif-level:integration a skos:Concept ;
	skos:prefLabel "Service Integration"@en ;
	skos:definition "Services should be governed to ensure integration, seamless execution, reuse of services and data, and development of new services and building blocks."@en ;
	skos:related eif-recommendation:governancestructure, eif-recommendation:agreements ;
	skos:inScheme eif-level: .


eif-principle:subsidiarity a skos:Concept ;
	skos:prefLabel "Subsidiarity and proportionality"@en ;
	skos:notation "1"^^xsd:integer ;
	skos:related eif-recommendation:alignment ;
	skos:inScheme eif-principle:  .

eif-principle:openness a skos:Concept ;
	skos:prefLabel "Openness"@en ;
	skos:notation "2"^^xsd:integer ;
	skos:related eif-recommendation:opendata, eif-recommendation:opensource, eif-recommendation:openspecifications ;
	skos:inScheme eif-principle:  .

eif-principle:transparency a skos:Concept ;
	skos:prefLabel "Transparency"@en ;
	skos:notation "3"^^xsd:integer ;
	skos:related eif-recommendation:interfaces ;
	skos:inScheme eif-principle:  .

eif-principle:reusability a skos:Concept ;
	skos:prefLabel "Reusability"@en ;
	skos:notation "4"^^xsd:integer ;
	skos:related eif-recommendation:solutionreuse, eif-recommendation:datareuse ;
	skos:inScheme eif-principle:  .

eif-principle:neutrality a skos:Concept ;
	skos:prefLabel "Technological neutrality and data portability"@en ;
	skos:notation "5"^^xsd:integer ;
	skos:related eif-recommendation:neutrality, eif-recommendation:dataportability ;
	skos:inScheme eif-principle:  .

eif-principle:usercentricity a skos:Concept ;
	skos:prefLabel "User-centricity"@en ;
	skos:notation "6"^^xsd:integer ;
	skos:related eif-recommendation:multichannel, eif-recommendation:spoc, eif-recommendation:userinvolvement, eif-recommendation:onceonly ;
	skos:inScheme eif-principle:  .

eif-principle:inclusion a skos:Concept ;
	skos:prefLabel "Inclusion and accessibility"@en ;
	skos:notation "7"^^xsd:integer ;
	skos:related eif-recommendation:accessibility ;
	skos:inScheme eif-principle:  .

eif-principle:security a skos:Concept ;
	skos:prefLabel "Security and privacy"@en ;
	skos:notation "8"^^xsd:integer ;
	skos:related eif-recommendation:privacy ;
	skos:inScheme eif-principle:  .

eif-principle:multilingualism a skos:Concept ;
	skos:prefLabel "Multilingualism"@en ;
	skos:notation "9"^^xsd:integer ;
	skos:related eif-recommendation:multilingualism ;
	skos:inScheme eif-principle:  .

eif-principle:simplification a skos:Concept ;
	skos:prefLabel "Administrative simplification"@en ;
	skos:notation "10"^^xsd:integer ;
	skos:related eif-recommendation:simplification ;
	skos:inScheme eif-principle:  .

eif-principle:preservation a skos:Concept ;
	skos:prefLabel "Preservation of information"@en ;
	skos:notation "11"^^xsd:integer ;
	skos:related eif-recommendation:preservation ;
	skos:inScheme eif-principle:  .

eif-principle:assessment a skos:Concept ;
	skos:prefLabel "Assessment of effectiveness and efficiency"@en ;
	skos:notation "12"^^xsd:integer ;
	skos:related eif-recommendation:assessment ;
	skos:inScheme eif-principle:  .


eif-recommendation:alignment a skos:Concept ;
	skos:prefLabel "NIFO alignment"@en ;
	skos:notation "1"^^xsd:integer ;
	skos:definition "Ensure that national interoperability frameworks and interoperability strategies are aligned with the EIF and, if needed, tailor and extend them to address the national context and needs."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:opendata a skos:Concept ;
	skos:prefLabel "Open data"@en ;
	skos:notation "2"^^xsd:integer ;
	skos:definition "Publish the data you own as open data unless certain restrictions apply."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:opensource a skos:Concept ;
	skos:prefLabel "Open source"@en ;
	skos:notation "3"^^xsd:integer ;
	skos:definition "Ensure a level playing field for open source software and demonstrate active and fair consideration of using open source software, taking into account the total cost of ownership of the solution."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:openspecifications a skos:Concept ;
	skos:prefLabel "Open specifications"@en ;
	skos:notation "4"^^xsd:integer ;
	skos:definition "Give preference to open specifications, taking due account of the coverage of functional needs, maturity and market support and innovation."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:interfaces a skos:Concept ;
	skos:prefLabel "External interfaces"@en ;
	skos:notation "5"^^xsd:integer ;
	skos:definition "Ensure internal visibility and provide external interfaces for European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:solutionreuse a skos:Concept ;
	skos:prefLabel "Solution reuse"@en ;
	skos:notation "6"^^xsd:integer ;
	skos:definition "Reuse and share solutions, and cooperate in the development of joint solutions when implementing European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:datareuse a skos:Concept ;
	skos:prefLabel "Solution reuse"@en ;
	skos:notation "7"^^xsd:integer ;
	skos:definition "Reuse and share information and data when implementing European public services, unless certain privacy or confidentiality restrictions apply."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:neutrality a skos:Concept ;
	skos:prefLabel "Technological neutrality"@en ;
	skos:notation "8"^^xsd:integer ;
	skos:definition "Do not impose any technological solutions on citizens, businesses and other administrations that are technology-specific or disproportionate to their real needs."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:dataportability a skos:Concept ;
	skos:prefLabel "Data portability"@en ;
	skos:notation "9"^^xsd:integer ;
	skos:definition "Ensure data portability, namely that data is easily transferable between systems and applications supporting the implementation and evolution of European public services without unjustified restrictions, if legally possible."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:multichannel a skos:Concept ;
	skos:prefLabel "Multi-channel"@en ;
	skos:notation "10"^^xsd:integer ;
	skos:definition "Use multiple channels to provide the European public service, to ensure that users can select the channel that best suits their needs."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:spoc a skos:Concept ;
	skos:prefLabel "SPOC"@en ;
	skos:notation "11"^^xsd:integer ;
	skos:definition "Provide a single point of contact in order to hide internal administrative complexity and facilitate usersâ€™ access to European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:userinvolvement a skos:Concept ;
	skos:prefLabel "User involvement"@en ;
	skos:notation "12"^^xsd:integer ;
	skos:definition "Put in place mechanisms to involve users in analysis, design, assessment and further development of European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:onceonly a skos:Concept ;
	skos:prefLabel "Once-only"@en ;
	skos:notation "13"^^xsd:integer ;
	skos:definition "As far as possible under the legislation in force, ask users of European public services once-only and relevant-only information."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:accessibility a skos:Concept ;
	skos:prefLabel "Accessibility"@en ;
	skos:notation "14"^^xsd:integer ;
	skos:definition "Ensure that all European public services are accessible to all citizens, including persons with disabilities, the elderly and other disadvantaged groups. For digital public services, public administrations should comply with e-accessibility specifications that are widely recognised at European or international level."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:privacy a skos:Concept ;
	skos:prefLabel "Privacy framework"@en ;
	skos:notation "15"^^xsd:integer ;
	skos:definition "Define a common security and privacy framework and establish processes for public services to ensure secure and trustworthy data exchange between public administrations and in interactions with citizens and businesses."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:multilingualism a skos:Concept ;
	skos:prefLabel "Multilingualism"@en ;
	skos:notation "16"^^xsd:integer ;
	skos:definition "Use information systems and technical architectures that cater for multilingualism when establishing a European public service. Decide on the level of multilingualism support based on the needs of the expected users."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:simplification a skos:Concept ;
	skos:prefLabel "Simplification"@en ;
	skos:notation "17"^^xsd:integer ;
	skos:definition "Simplify processes and use digital channels whenever appropriate for the delivery of European public services, to respond promptly and with high quality to users' requests and reduce the administrative burden on public administrations, businesses and citizens."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:preservation a skos:Concept ;
	skos:prefLabel "Long-term preservation"@en ;
	skos:notation "18"^^xsd:integer ;
	skos:definition "Formulate a long-term preservation policy for information related to European public services and especially for information that is exchanged across borders."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:assessment a skos:Concept ;
	skos:prefLabel "Assessment"@en ;
	skos:notation "19"^^xsd:integer ;
	skos:definition "Evaluate the effectiveness and efficiency of different interoperability solutions and technological options considering user needs, proportionality and balance between costs and benefits."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:holisticgovernance a skos:Concept ;
	skos:prefLabel "Holistic governance"@en ;
	skos:notation "20"^^xsd:integer ;
	skos:definition "Ensure holistic governance of interoperability activities across administrative levels and sectors."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:standardsprocess a skos:Concept ;
	skos:prefLabel "Standards process"@en ;
	skos:notation "21"^^xsd:integer ;
	skos:definition "Put in place processes to select relevant standards and specifications, evaluate them, monitor their implementation, check compliance and test their interoperability."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:standardsassessment a skos:Concept ;
	skos:prefLabel "Standards assessment"@en ;
	skos:notation "22"^^xsd:integer ;
	skos:definition "Use a structured, transparent, objective and common approach to assessing and selecting standards and specifications. Take into account relevant EU recommendations and seek to make the approach consistent across borders."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:standardscatalogues a skos:Concept ;
	skos:prefLabel "Standards catalogues"@en ;
	skos:notation "23"^^xsd:integer ;
	skos:definition "Consult relevant catalogues of standards, specifications and guidelines at national and EU level, in accordance with your NIF and relevant DIFs, when procuring and developing ICT solutions."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:standardsparticipation a skos:Concept ;
	skos:prefLabel "Participation"@en ;
	skos:notation "24"^^xsd:integer ;
	skos:definition "Actively participate in standardisation work relevant to your needs to ensure your requirements are met."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:governancestructure a skos:Concept ;
	skos:prefLabel "Governance structure"@en ;
	skos:notation "25"^^xsd:integer ;
	skos:definition "Ensure interoperability and coordination over time when operating and delivering integrated public services by putting in place the necessary governance structure."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:agreements a skos:Concept ;
	skos:prefLabel "Interoperability agreements"@en ;
	skos:notation "26"^^xsd:integer ;
	skos:definition "Establish interoperability agreements in all layers, complemented by operational agreements and change management procedures."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:legislation a skos:Concept ;
	skos:prefLabel "Digital legislation"@en ;
	skos:notation "27"^^xsd:integer ;
	skos:definition "Ensure that legislation is screened by means of 'interoperability checks', to identify any barriers to interoperability. When drafting legislation to establish a European public service, seek to make it consistent with relevant legislation, perform a â€˜digital checkâ€™ and consider data protection requirements."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:processalignment a skos:Concept ;
	skos:prefLabel "Process alignment"@en ;
	skos:notation "28"^^xsd:integer ;
	skos:definition "Document your business processes using commonly accepted modelling techniques and agree on how these processes should be aligned to deliver a European public service."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:relationships a skos:Concept ;
	skos:prefLabel "Organisational relationships"@en ;
	skos:notation "29"^^xsd:integer ;
	skos:definition "Clarify and formalise your organisational relationships for establishing and operating European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:datamanagement a skos:Concept ;
	skos:prefLabel "Data management"@en ;
	skos:notation "30"^^xsd:integer ;
	skos:definition "Perceive data and information as a public asset that should be appropriately generated, collected, managed, shared, protected and preserved."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:informationmanagement a skos:Concept ;
	skos:prefLabel "Information management strategy"@en ;
	skos:notation "31"^^xsd:integer ;
	skos:definition "Put in place an information management strategy at the highest possible level to avoid fragmentation and duplication. Management of metadata, master data and reference data should be prioritised."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:communities a skos:Concept ;
	skos:prefLabel "Communities"@en ;
	skos:notation "32"^^xsd:integer ;
	skos:definition "Support the establishment of sector-specific and cross-sectoral communities that aim to create open information specifications and encourage relevant communities to share their results on national and European platforms."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:openspecifications2 a skos:Concept ;
	skos:prefLabel "Open specifications"@en ;
	skos:notation "33"^^xsd:integer ;
	skos:definition "Use open specifications, where available, to ensure technical interoperability when establishing European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:servicesmodel a skos:Concept ;
	skos:prefLabel "Services model"@en ;
	skos:notation "34"^^xsd:integer ;
	skos:definition "Use the conceptual model for European public services to design new services or reengineer existing ones and reuse, whenever possible, existing service and data components."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:loosecoupling a skos:Concept ;
	skos:prefLabel "Loose coupling"@en ;
	skos:notation "35"^^xsd:integer ;
	skos:definition "Decide on a common scheme for interconnecting loosely coupled service components and put in place and maintain the necessary infrastructure for establishing and maintaining European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:sharedservices a skos:Concept ;
	skos:prefLabel "Shared services"@en ;
	skos:notation "36"^^xsd:integer ;
	skos:definition "Develop a shared infrastructure of reusable services and information sources that can be used by all public administrations."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:authenticsources a skos:Concept ;
	skos:prefLabel "Authentic sources"@en ;
	skos:notation "37"^^xsd:integer ;
	skos:definition "Make authoritative sources of information available to others while implementing access and control mechanisms to ensure security and privacy in accordance with the relevant legislation."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:documentation a skos:Concept ;
	skos:prefLabel "Documentation"@en ;
	skos:notation "38"^^xsd:integer ;
	skos:definition "Develop interfaces with base registries and authoritative sources of information, publish the semantic and technical means and documentation needed for others to connect and reuse available information."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:metadata a skos:Concept ;
	skos:prefLabel "Metadata"@en ;
	skos:notation "39"^^xsd:integer ;
	skos:definition "Match each base registry with appropriate metadata including the description of its content, service assurance and responsibilities, the type of master data it keeps, conditions of access and the relevant licences, terminology, a glossary, and information about any master data it uses from other base registries."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:quality a skos:Concept ;
	skos:prefLabel "Quality assurance"@en ;
	skos:notation "40"^^xsd:integer ;
	skos:definition "Create and follow data quality assurance plans for base registries and related master data."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:opendataprocesses a skos:Concept ;
	skos:prefLabel "Open data processes"@en ;
	skos:notation "41"^^xsd:integer ;
	skos:definition "Establish procedures and processes to integrate the opening of data in your common business processes, working routines, and in the development of new information systems."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:machinereadable a skos:Concept ;
	skos:prefLabel "Machine-readable data"@en ;
	skos:notation "42"^^xsd:integer ;
	skos:definition "Publish open data in machine-readable, non-proprietary formats. Ensure that open data is accompanied by high quality, machine-readable metadata in non-proprietary formats, including a description of their content, the way data is collected and its level of quality and the licence terms under which it is made available. The use of common vocabularies for expressing metadata is recommended."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:openlicenses a skos:Concept ;
	skos:prefLabel "Open licenses"@en ;
	skos:notation "43"^^xsd:integer ;
	skos:definition "Communicate clearly the right to access and reuse open data. The legal regimes for facilitating access and reuse, such as licences, should be standardised as much as possible."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:publiccatalogues a skos:Concept ;
	skos:prefLabel "Public catalogues"@en ;
	skos:notation "44"^^xsd:integer ;
	skos:definition "Put in place catalogues of public services, public data, and interoperability solutions and use common models for describing them."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:externalsources a skos:Concept ;
	skos:prefLabel "External sources"@en ;
	skos:notation "45"^^xsd:integer ;
	skos:definition "Where useful and feasible to do so, use external information sources and services while developing European public services."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:riskmanagement a skos:Concept ;
	skos:prefLabel "Risk management"@en ;
	skos:notation "46"^^xsd:integer ;
	skos:definition "Consider the specific security and privacy requirements and identify measures for the provision of each public service according to risk management plans."@en ;
	skos:inScheme eif-recommendation: .

eif-recommendation:trustservices a skos:Concept ;
	skos:prefLabel "Trust services"@en ;
	skos:notation "47"^^xsd:integer ;
	skos:definition "Use trust services according to the Regulation on eID and Trust Services as mechanisms that ensure secure and protected data exchange in public services."@en ;
	skos:inScheme eif-recommendation: .
